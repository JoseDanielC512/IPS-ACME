<h2 class="center-align">Registrate</h2>
<div class="row">
  <div class="col s4"></div>
  <div class="col s4 14 offset-14">
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>

  <div class="field">
    <%= f.label :nombre %><br />
    <%= f.text_field :nombre, autofocus: true, autocomplete: "Nombre" %>
  </div>

  <div class="field">
    <%= f.label :apellido %><br />
    <%= f.text_field :apellido, autofocus: true, autocomplete: "Apellido" %>
  </div>

  <div class="field">
    <%= f.label :cedula %><br />
    <%= f.text_field :cedula, autofocus: true, autocomplete: "Cedúla" %>
  </div>

  <div class="field">
    <%= f.label :telefono %><br />
    <%= f.text_field :telefono, autofocus: true, autocomplete: "Telefono" %>
  </div>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :password %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> caracteres minimos)</em>
    <% end %><br />
    <%= f.password_field :password , autocomplete: "nueva contraseña" %>
  </div>

   <div class="field">
      <%= f.label :confirme_contraseña %><br />
      <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
    </div>

  <div class="actions center-align">
    <%= f.submit "Registrar" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
</div>
</div>
