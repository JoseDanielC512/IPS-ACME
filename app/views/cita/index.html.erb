<p id="notice"><%= notice %></p>

<h1 class="center-align">Mis citas</h1>
<div class="row">
  <div class="col s4"></div>
  <div class="col s4 14 offset-14">
<table>
  <thead>
    <tr>
      <th>Fecha</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @citum.each do |citum| %>
      <tr>
        <td><%= citum.Fecha %></td>
        <td><%= link_to 'Ver', citum %></td>
        <td><%= link_to 'Aplazar', edit_citum_path(citum) %></td>
        <td><%= link_to 'Eliminar', citum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<div class="center-align">
<%= link_to 'nueva cita', new_citum_path %>
</div>
</div>
</div>
